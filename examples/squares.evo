# this program displays the square of every number up to 100
push 0
<- ctr 

loop: push ctr                  
    print                       # print the value of ctr
    print_p " -> "
    * dup                       # calculate the square of ctr
    println_p                   # print the square
    + 1 ctr                     # push ctr +1 to the stack
    set ctr 

push ctr 101                # jump to loop, if counter is not equal to 101
j!= loop                         