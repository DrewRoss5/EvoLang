# this program displays the square of every number up to 100
set ctr 0

loop: push ctr
    print_p 
    print_p " -> "
    * dup ctr                   # calculate the square of ctr
    println                     # print the square
    + 1 ctr             # push ctr +1 to the stack
    set ctr 
              
jif loop neq ctr 101
